{
  "name": "@dallmo/ipsum",
  "description": "deno module, generating fake texts.",
  "version": "1.3.1",
  "exports": "./mod.ts",
  "tasks": {
    "run": "deno run -A app.ts",
    "dev": "deno run --watch app.ts",
    "test": "cd ./test; deno test --allow-read",
    "test-jsr": "cd ./test; deno run --allow-read ./test-via-jsr.ts",
